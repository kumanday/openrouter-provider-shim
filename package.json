{
  "name": "openrouter-provider-shim",
  "version": "0.1.0",
  "description": "Local npx shim that injects OpenRouter provider routing for Claude Code and OpenAI-compatible harnesses",
  "type": "module",
  "bin": {
    "openrouter-provider-shim": "./dist/cli.js"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "build": "tsup src/cli.ts --format esm --dts --clean",
    "dev": "node --watch dist/cli.js serve --port 8787",
    "lint": "eslint .",
    "test": "vitest"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.6.0",
    "vitest": "^2.0.0",
    "@types/node": "^22.0.0",
    "eslint": "^9.0.0"
  },
  "keywords": [
    "openrouter",
    "claude-code",
    "openai",
    "proxy",
    "provider-routing",
    "shim"
  ],
  "author": "",
  "license": "MIT"
}
